class Point2D {     
    int x; // Abscisse     
    int y; // Ordonnee      
    void diag(int a) {         
        x = x + a;         
        y = y + a;     
    }      
    Point2D get() {        
        return this;     
    } 
} 

class Point3D extend Point2D {     
    int z; // Hauteur      
    void diag(int a) {         
        x = x + a;         
        y = y + a;         
        z = z + a;     
    } 
} 