    int x = 3;
}
{
    A a = new A();
    B b = new B();
    C c = new C();
    A b2 = new B();
    B c2 = new C();

    println("a.x = ", a.x);
    println("b.x = ", b.x);
    println("c.x = ", c.x);
    println("b2.x = ", b2.x);
    println("c2.x = ", c2.x);
}