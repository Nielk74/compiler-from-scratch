class A {
    int x = 22;
    float y = 12;
    int z = 12;
}
{
    A a = new A();
    if (a.x > a.y) {
        println(a.x, " > ", a.y);
    } else {
        println(a.x, " <= ", a.y);
    }

    if (a.x >= a.y) {
        println(a.x, " >= ", a.y);
    } else {
        println(a.x, " < ", a.y);
    }

    if (a.y < a.x) {
        println(a.y, " < ", a.x);
    } else {
        println(a.y, " >= ", a.x);
    }

    if (a.y <= a.x) {
        println(a.y, " <= ", a.x);
    } else {
        println(a.y, " > ", a.x);
    }

    if (a.x != a.y) {
        println(a.x, " != ", a.y);
    } else {
        println(a.x, " == ", a.y);
    }

    if (a.z == a.y) {
        println(a.z, " == ", a.y);
    } else {
        println(a.z, " != ", a.y);
    }

}