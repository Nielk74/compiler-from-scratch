{
    int x = 1;
    int y = 2;
    int a = 1 - (y + x);
    int b = 1 - y + x;
    float c = (1 - y) / y;
    int d = (1 - x) * y;
    int e = (x + y) * (3 % 2);
    println(a);
    println(b);
    println(c);
    println(d);
    println(e);
}