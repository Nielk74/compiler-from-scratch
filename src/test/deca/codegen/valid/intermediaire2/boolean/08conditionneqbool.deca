// Description:
// Test de l'assignation d'expression booléenne dans une variable booléenne, dans une initialisation,
// et de la négation de littérals booléens, et d'expression booléenne
//
// Resultats:
//  (true && (5.5 < 10)) || (!true == !(true && false))
//  !((true && (15.5 < 10)) || (!true == !(true && false)))
//
// Historique:
//    crée le 10/06/2022
 {
 int y = 10;
 boolean x = (true && (5.5 < y)) || (!true == !(true && false));
 int a = 10;
 boolean b = (true && (15.5 < a)) || (!true == !(true && false));
 if(x){
    println("(true && (5.5 < 10)) || (!true == !(true && false))");
 }else{
    println("!((true && (5.5 < 10)) || (!true == !(true && false)))");
 }

 if(b){
    println("(true && (15.5 < 10)) || (!true == !(true && false))");
 }else{
    println("!((true && (15.5 < 10)) || (!true == !(true && false)))");
 }
 }
