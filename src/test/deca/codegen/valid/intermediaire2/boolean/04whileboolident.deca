// Description:
// Test des variables booléennes pour les conditions dans un while
//
// Resultats:
//  true && (5 < 10)
//  !(true && (10 < 10))
//  !(false && (5 < 10))
//
// Historique:
//    crée le 10/06/2022
{
int x = 5;
boolean y = true;
int a = 5;
boolean b = false;
while(y && (x < 10)){
  println("true && (5 < 10)");
  x = 10;
}
println("!(true && (10 < 10))");

while(b && (a < 10)){
  println("false && (5 < 10)");
  a = 10;
}
println("!(false && (5 < 10))");
}
