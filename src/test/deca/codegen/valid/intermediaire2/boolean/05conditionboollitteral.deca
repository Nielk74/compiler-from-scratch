// Description:
// Test des littérals booléens pour les conditions dans un if, avec des or
//
// Resultats:
//  (true && (4 == 5)) || false || (true == true)
//  !((true && (4 == 5)) || false || (true == false))
//
// Historique:
//    crée le 10/06/2022
{
if((true && (4 == 5)) || false || (true == true)){
  println("(true && (4 == 5)) || false || (true == true)");
}else{
  println("!((true && (4 == 5)) || false || (true == true))");
}

//Exécute le else

if((true && (4 == 5)) || false || (true == false)){
  println("(true && (4 == 5)) || false || (true == false)");
}else{
  println("!((true && (4 == 5)) || false || (true == false))");
}
}
