// Description:
// Test des littérals booléen pour les conditions dans un while
//
// Resultats:
//  true && (5 < 10)
//  !(true && (10 < 10))
//  !(false && (5 < 10))
//
// Historique:
//    crée le 10/06/2022
{
int x = 5;
int y = 5;
while(true && (x < 10)){
  println("true && (5 < 10)");
  x = 10;
}
println("!(true && (10 < 10))");

while(false && (y < 10)){
  println("false && (y < 10)");
  y = 10;
}
println("!(false && (5 < 10))");
}
